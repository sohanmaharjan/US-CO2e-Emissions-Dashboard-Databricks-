{
  "datasets": [
    {
      "name": "84c85783",
      "displayName": "Location data",
      "queryLines": [
        "SELECT latitude,longitude,`GHG emissions mtons CO2e` as Emissions\r\n",
        "FROM emissions_data"
      ]
    },
    {
      "name": "ed548cc5",
      "displayName": "Emissions per person",
      "queryLines": [
        "SELECT\r\n",
        "  county_state_name,\r\n",
        "  population,\r\n",
        "  CAST(REPLACE(`GHG emissions mtons CO2e`, ',','') AS DOUBLE) / \r\n",
        "  NULLIF(CAST(REPLACE(population, ',','') AS DOUBLE),0)\r\n",
        "  AS emissions_per_person\r\n",
        "FROM\r\n",
        "  emissions_data\r\n",
        "ORDER BY\r\n",
        "  emissions_per_person DESC"
      ]
    },
    {
      "name": "1c4e34a2",
      "displayName": "Total Emissions per State",
      "queryLines": [
        "SELECT\r\n",
        "  state_abbr,\r\n",
        " SUM(CAST(REPLACE(`GHG emissions mtons CO2e`, ',','') AS DOUBLE))\r\n",
        "  AS TOTAL_EMISSIONS\r\n",
        "FROM\r\n",
        "  emissions_data\r\n",
        "GROUP BY\r\n",
        "  state_abbr\r\n",
        "ORDER BY\r\n",
        "  TOTAL_EMISSIONS DESC\r\n",
        "LIMIT 10"
      ]
    },
    {
      "name": "23aa802a",
      "displayName": "County Shaming",
      "queryLines": [
        "SELECT\r\n",
        "  county_state_name,\r\n",
        "  population,\r\n",
        "  CAST(REPLACE(`GHG emissions mtons CO2e`, ',','') AS DOUBLE)\r\n",
        "  AS Total_Emissions\r\n",
        "FROM\r\n",
        "  emissions_data\r\n",
        "ORDER BY\r\n",
        "  Total_Emissions DESC\r\n",
        "LIMIT 10"
      ]
    }
  ],
